$(document).ready(function(){
var lastPriceString = $('#special_price_box').text().replace('RP','').replace(/\./g, '').trim();
var lastPriceInt = parseInt(lastPriceString);

if($('.product__detail-badge').hasClass('no_mc')) {
    return;
} else if(lastPriceInt < 2500000){
var newPrice = lastPriceInt - lastPriceInt * 0.1;
$('#product-price-box .prod_pricebox_price').after('<div class="mobile-block mobile-block-mc"><div class="mobile-price2">' + newPrice.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' RP' + '</div></div>' );
}
else if(lastPriceInt >= 2500000) {
var newPrice = lastPriceInt - 250000;
$('#product-price-box .prod_pricebox_price').after('<div class="mobile-block mobile-block-mc"><div class="mobile-price2">' + newPrice.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' RP' + '</div></div>' );
}

function searchall(searchType){
var DEFAULT_TEXT = {};

	return DEFAULT_TEXT[searchType]; 
}

var sku = $('#pdtsku').text();
var newprice = searchall(sku);
if(newprice == undefined)
{
	return;
} else
{

$('#product-price-box .prod_pricebox_price').after('<div class="mobile-block"><div class="mobile-price">' + newprice + '</div></div>');	
}

});
})(jQuery);Rocket.helper.errorSafe("js from CMS with key:header2014-customer-care", function($){
$(document).ready(function(){

$('.product-card .product-card__price').each(function() {
var lastPriceString = $(this).text().replace('RP','').replace(/\./g, '').trim();
var lastPriceInt = parseInt(lastPriceString);
if(lastPriceInt < 2500000) {
var newPrice = lastPriceInt - lastPriceInt * 0.1;
$(this).after('<div class="mobile-block-mc"><div class="new-price2">' + newPrice.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' RP' + '</div></div>' );
}
else if (lastPriceInt  >= 2500000){
var lastPriceString = $(this).text().replace('RP','').replace(/\./g, '').trim();
var lastPriceInt = parseInt(lastPriceString);
var newPrice = lastPriceInt - 250000;
$(this).after('<div class="mobile-block-mc"><div class="new-price2">' + newPrice.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' RP' + '</div></div>' );
}

});

function searchall(searchType){
var DEFAULT_TEXT = {};

	
	return DEFAULT_TEXT[searchType]; 
}
$('.button-buy').each(function() {
var sku = $(this).attr('data-sku-simple');

var newprice = searchall(sku);
if(newprice == undefined)
{
	return;
} else
{

$(this).after('<div class="new-price discount ">' + newprice + '</div>');
	
}
});

});
