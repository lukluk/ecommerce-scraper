<script type="text/javascript">
var bxSlide_obj;
$(document).ready(function(){

  bxSlide_obj = $('.bxslide').bxSlider({
    mode: 'vertical',
    pagerCustom: '#bx-pager',
    auto:true,
    speed:500,
    slideWidth: 700,
    slideMargin: 10,
    autoDelay:20,
    autoHover:true,
    infiniteLoop:true
  });

/* Init countinue shopping click event - edit by Trung Le 26 March, 2014*/

  $(".icon-brand").click(function(){
    $(".icon-brand").removeClass("active");
    $(this).addClass("active");
  });

$('#btn-shopping').hover(function() {
    if ( $('#hdCartCount').length > 0 ) {
        $('#hdCartLink').trigger('mouseenter');
    } else {
        location.href = location.origin;
    }
});

 $("#bx-pager").on('mouseenter', 'div', function(e) {
      e.preventDefault();
      bxSlide_obj.goToSlide($(this).attr('data-slide-index'));
    });

  $("#bx-pager").mouseover(function(){
  bxSlide_obj.stopAuto();
  }).mouseleave(function(){
  bxSlide_obj.startAuto();
  });


/* mouse-over*/
 $("[id$='out-mo1']").mouseover(function(){
    $("[id$='tab1']").addClass("over1");
    }).mouseout(function(){
    $("[id$='tab1']").removeClass("over1");
  });

  $("[id$='out-mo2']").mouseover(function(){
    $("[id$='tab2']").addClass("over2");
    }).mouseout(function(){
    $("[id$='tab2']").removeClass("over2");
  });

  $("[id$='out-mo3']").mouseover(function(){
    $("[id$='tab3']").addClass("over3");
    }).mouseout(function(){
    $("[id$='tab3']").removeClass("over3");
  });

  $("[id$='out-mo4']").mouseover(function(){
    $("[id$='tab4']").addClass("over4");
    }).mouseout(function(){
    $("[id$='tab4']").removeClass("over4");
  });
  
  $("[id$='out-mo5']").mouseover(function(){
    $("[id$='tab5']").addClass("over5");
    }).mouseout(function(){
    $("[id$='tab5']").removeClass("over5");
  });

  $("[id$='out-mo6']").mouseover(function(){
    $("[id$='tab6']").addClass("over6");
    }).mouseout(function(){
    $("[id$='tab6']").removeClass("over6");
  });

  $("[id$='out-mo7']").mouseover(function(){
    $("[id$='tab7']").addClass("over7");
    }).mouseout(function(){
    $("[id$='tab7']").removeClass("over7");
  });

  $("[id$='out-mo8']").mouseover(function(){
