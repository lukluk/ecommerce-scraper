<script type="text/javascript">
var bxSlide_obj;
$(document).ready(function(){

  bxSlide_obj = $('.bxslide').bxSlider({
    mode: 'vertical',
    pagerCustom: '#bx-pager',
    auto:true,
    speed:500,
    slideWidth: 700,
    slideMargin: 10,
    autoDelay:20,
    autoHover:true,
    infiniteLoop:true
  });

/* Init countinue shopping click event - edit by Trung Le 26 March, 2014*/

  $(".icon-brand").click(function(){
    $(".icon-brand").removeClass("active");
    $(this).addClass("active");
  });

$('#btn-shopping').hover(function() {
    if ( $('#hdCartCount').length > 0 ) {
        $('#hdCartLink').trigger('mouseenter');
    } else {
        location.href = location.origin;
    }
});

 $("#bx-pager").on('mouseenter', 'div', function(e) {
      e.preventDefault();
      bxSlide_obj.goToSlide($(this).attr('data-slide-index'));
    });

  $("#bx-pager").mouseover(function(){
  bxSlide_obj.stopAuto();
  }).mouseleave(function(){
  bxSlide_obj.startAuto();
  });


/* mouse-over*/
 $("[id$='out-mo1']").mouseover(function(){
    $("[id$='tab1']").addClass("over1");
    }).mouseout(function(){
    $("[id$='tab1']").removeClass("over1");
  });

  $("[id$='out-mo2']").mouseover(function(){
