} else if(lastPriceInt < 2500000){
var newPrice = lastPriceInt - lastPriceInt * 0.1;
$('#product-price-box .prod_pricebox_price').after('<div class="mobile-block mobile-block-mc"><div class="mobile-price2">' + newPrice.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' RP' + '</div></div>' );
}
else if(lastPriceInt >= 2500000) {
var newPrice = lastPriceInt - 250000;
$('#product-price-box .prod_pricebox_price').after('<div class="mobile-block mobile-block-mc"><div class="mobile-price2">' + newPrice.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' RP' + '</div></div>' );
}

function searchall(searchType){
var DEFAULT_TEXT = {};

	return DEFAULT_TEXT[searchType]; 
}

var sku = $('#pdtsku').text();
var newprice = searchall(sku);
if(newprice == undefined)
{
	return;
} else
{

$('#product-price-box .prod_pricebox_price').after('<div class="mobile-block"><div class="mobile-price">' + newprice + '</div></div>');	
}

});
})(jQuery);Rocket.helper.errorSafe("js from CMS with key:header2014-customer-care", function($){
