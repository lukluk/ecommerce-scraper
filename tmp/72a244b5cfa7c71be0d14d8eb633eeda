<script>
var hotDeal = {

        el: $('.hot-deal-left-container'),
        ls: $('.hdc-list-hot-deal-product'),

        hourly: [
            '8:00 AM',
            '9:00 AM',
            '10:00 AM',
            '11:00 AM',
            '12:00 PM',
            '1:00 PM',
            '2:00 PM',
            '3:00 PM',
            '4:00 PM',
            '5:00 PM',
            '6:00 PM',
            '7:00 PM',
            '8:00 PM',
            '9:00 PM',
            '10:00 PM',
            '11:00 PM',
            '12:00 PM'
        ],

        getProductUpcomming: function(targetDate) {
            var today = new Date(),
                currentDay = today.getDate(),
                currentHour = today.getHours(),
                targetDay = targetDate.getDate(),
                productPosition, productItem;

            /*If today is not the 11th, do nothing*/
            if (targetDay !== currentDay) {
                return false;
            }

            /*Find product with upcomming hour*/
            productPosition = 'div[data-hour="' + currentHour + '"]';
            productItem = this.ls.find(productPosition);

            /**/
            /*if (productItem.hasClass('active')) {
                return false;
            }*/

            return productItem;
        },

        getFirstProduct: function() {
            return $('.hdc-list-hot-deal-product').find('.hdc-item').first();
        },

        setProductHourly: function() {
            var productList = $('.hdc-list-hot-deal-product').find('.hdc-item'),
                index = 8,
                classIndex = 0,
                buyNowWrapper = $('#buy-wrapper').find('.data-buynow');

            $.each(productList, function() {
                var that = $(this);
                that.attr('data-hour', index).attr('data-index', classIndex).addClass('item-' + classIndex);
                /*var buynowItem = $(buyNowWrapper[classIndex]);
                if (buynowItem.length > 0) {
                	buynowItem.addClass('item-' + classIndex);
                }*/
                index++;
                classIndex++;
            });
        },

        bindClickItem: function() {
            /*var bodyEl = $('body.mobile');
            if (bodyEl.length > 0) {
                return false;
            }*/
            var productList = $('.hdc-list-hot-deal-product').find('.hdc-item');
            if (productList.length > 0) {
                productList.on('click', function() {
                    var that = $(this),
                        aTag = that.find('.hdc-wrap');
                    window.location.href = aTag.attr('href');
                });
            }
        },

        updateProductContent: function() {
            var me = this, productList = $('.hdc-list-hot-deal-product').find('.hdc-item'),
                productDataList = $('.hdc-hotdeal-image-data'),
                classIndex = 0,
                currency = '';

            if (typeof store !== 'undefined' && store && store.richRelevance && store.richRelevance.currency) {
                currency = store.richRelevance.currency;
            }
            $.each(productList, function() {
                var that = $(this),
                    itemWrap = that.find('.hdc-wrap'),
                    itemContent = that.find('.hdc-item-content'),
                    className = 'item-' + classIndex;

                me.updateProductHourlyLabel(that, classIndex);

                if(that.hasClass(className) === true) {
                    var itemData = productDataList.find('.' + className);
                    if (itemWrap.length > 0 && itemContent.length > 0 && itemData.length > 0) {

                        itemWrap.append(me.getImageFromCMS(itemData.data('simage')));

                        itemContent.attr('data-image', itemData.data('bimage')).attr('data-sku', itemData.data('sku')).attr('data-sku-simple', itemData.data('skusimple'));

                        me.updateProductDiscount(itemContent, currency, that);
                    }
                }
                me.makeCurrencyFormatEachItem(that);
                classIndex++;
            });
        },

        makeCurrencyFormatEachItem: function(productItem) {
        	var me = this, productPrice = productItem.find('.c-value');
        	if (productItem.length > 0 && productPrice.length > 0) {
        		productPrice.html(me.makeCurrencyFormat(productPrice.html()));
        	}
        },

        createTempProductItem: function(text) {
            var targetEl = this.ls.find('.itmSimpleList'),
                    liEl = '';
            if (targetEl.length > 0 && typeof store !== 'undefined' && store && store.venture === "MY") {
                liEl = $('<li>').html(text).attr('class', 'unit hdc-special-element');
                targetEl.prepend(liEl);
            }
        },

        updateProductHourlyLabel: function(productItem, index) {
            if(productItem.length > 0 && this.hourly.length > 0) {
                var hourElement = productItem.find('.h-hourly'),
                    preciseElement = productItem.find('.h-precise');

                if (hourElement.length > 0 && preciseElement.length > 0 && this.hourly[index]) {
                    var temp = this.hourly[index].split(' ');
                    if (temp.length > 1) {
                        hourElement.html(temp[0]);
                        preciseElement.html(temp[1]);
                    }
                }
            }
        },
		
		displaySizePopup: function(flag) {
        	var buyNow = this.el.find('.data-buynow'),
        		popUp = this.el.find('.size-select-box');
        	buyNow.on('click', function() {
        		if (flag === true) {
        			popUp.removeClass('hide');
        			buyNow.addClass('buynow-inactive');
        		}
        	});
        },
        getImageFromCMS: function(path) {
            return ['<img class="hdc-product-image" src="', path, '" alt="Hot Deal">'].join('');
        },

        updateProductDiscount: function(itemContent, currency, productItem) {
            if (itemContent.length > 0) {
                var price = itemContent.data('price'),
                    oldprice = itemContent.data('oldprice'),
                    oldpriceNoCurrency = 0,
                    temp = null,
                    discount = 0;

                var itemCurrency = productItem.find('.c-currency'),
                itemDiscount = productItem.find('.d-discount-percent');
                if (itemCurrency.length > 0) {
                    itemCurrency.html(currency);
                }

                if (price > 0 && oldprice.length > 0) {
                    temp = oldprice.split(' ');
                    if (temp.length > 1) {
                    	var tempPrice = 0;
                        if (typeof store !== 'undefined' && store && (store.venture==="VN" || store.venture==="TH")) {
                        	tempPrice = temp[0];
                        } else {
                        	tempPrice = temp[1];
                        }

                        if (typeof store !== 'undefined' && store && (store.venture==="VN" || store.venture==="ID")) {
                        	oldpriceNoCurrency = Number(tempPrice.replace('.', '').replace('.', '').replace('.', ''));
                        } else {
                        	oldpriceNoCurrency = Number(tempPrice.replace(',', '').replace(',', '').replace(',', ''));
                        }

                        itemContent.attr('data-currency', currency);
                    }
                    if (oldpriceNoCurrency > 0 && price < oldpriceNoCurrency) {
                        discount = Math.round(((oldpriceNoCurrency - price) / oldpriceNoCurrency) * 100);
                    }

                    if (discount > 0) {
                        itemContent.attr('data-discount', discount + '%');
                        if (itemDiscount.length > 0) {
                            itemDiscount.html(discount + '%');
                        }
                    }
                }
            }
        },

        showProductClock: function() {
            this.el.removeClass('hdc-startin').find('.hide').removeClass('hide');
        },

        render: function(productItem, notStarted) {
            var me = this.el,
                parent = this,
                titleElement = me.find('.data-title'),
                ptitleElement = me.find('.data-ptitle'),
                imageElement = me.find('.data-pimage'),
                currencyElement = me.find('.data-currency'),
                priceElement = me.find('.data-price'),
                oldPriceElement = me.find('.data-oldprice'),
                discountElement = me.find('.data-discount'),
                buyNowElement = me.find('.data-buynow');

            me.addClass('hide');
            if (productItem.length > 0) {
                var productData = productItem.find('.hdc-item-content').data(), currency = '';
                //titleElement.html(productData.title);
                ptitleElement.html(productData.ptitle);
                imageElement.attr('src', productData.image);
                imageElement.attr('alt', productData.alt);

                if (productData.currency.length <= 0) {
                    if (typeof store !== 'undefined' && store && store.richRelevance && store.richRelevance.currency) {
                        currency = store.richRelevance.currency;
                    }
                } else {
                    currency = productData.currency;
                }
                currencyElement.html(currency);
                priceElement.html(parent.makeCurrencyFormat(productData.price));
                if (productData.oldprice.length <= 0) {
                    oldPriceElement.closest('.old-price').addClass('hide');
                } else {
                    oldPriceElement.html(productData.oldprice);
                }
                if (productData.discount.length <= 0) {
                    discountElement.closest('.percent').addClass('hide');
                } else {
                    discountElement.html(productData.discount);
                }
				if (typeof productData.sizes === 'object' && Object.keys(productData.sizes).length > 0) {
            		$('.buynow-refresh').html('<div data-sku="" data-sku-simple="" data-final-price="" class="btn-buy btn-add-to-cart  data-buynow hide"> <span class="btn-text">Beli Sekarang</span></div>');
        			parent.generateProductVariant(productItem, productData.sizes);
	        		hotDeal.displaySizePopup(true);
	        	} else {
	        		buyNowElement = me.find('.data-buynow');
	        		buyNowElement.attr('data-sku', productData.sku);
	                buyNowElement.attr('data-sku-simple', productData.skuSimple);
    	            buyNowElement.attr('data-final-price', productData.price);
		        	buyNowElement.off('click');
                	var quickbuy = new Rocket.plugin.quickbuy(buyNowElement, {});
	        	}

                /*Update SKU buy now on mobile*/
                var mobileSku = me.find('#configSku'),
                    mobileSkuSimple = me.find('#selectedSku');

                if (mobileSku) {
                    me.find('#configSku').val(productData.sku);
                    me.find('#selectedSku').val(productData.skuSimple);
                }

                parent.activeProductItem(productItem);
                /*if (parent.ismobile() === false) {
                    parent.goToSlide(productItem);
                }*/
                if (notStarted === false) {
                	parent.displayOutOfStock(productItem);
                }
                parent.highlightProductItem(productItem);
                parent.productUrlProcess(productItem);
                parent.updateHourlyOnHeader(productItem);
                parent.updateProductItemDescription(productItem);
                /*Select size and add to cart*/
                parent.sizeSelectedForBuy();
                if (parent.ismobile() === true) {
                    setTimeout(function() {
                        parent.highlightBoxSize(productItem);
                    }, 4000);


                    parent.makeProductDescriptionStyle();
                }
            }
            me.removeClass('hide');
            this.ls.css({'visibility': 'visible'});
        },
generateProductVariant: function(activeProduct, sizes) {
        	var _html = '';
        	if (activeProduct.length > 0) {
        		var keys = Object.keys(sizes);
        		for (var i = 0; i < keys.length; i++) {
        			var sku = keys[i].split('-')[0],
        				skusimple = keys[i],
        				label = sizes[keys[i]],
        				liElement = ['<li class="quickbuySelectedSize size-li" ',
        							'data-sku="', sku, '" ',
        							'data-sku-simple="', skusimple, '" ',
        							'data-final-price="" >',
        							label,
        							'</li>'
        							].join('');
        			_html = _html + liElement;

        		}
        	}

        	var productSize = $('.size-select-box').find('.size-ul');
        	if (productSize.length > 0 && _html.length > 0) {
        		productSize.html(_html);

        		var elements = $('.quickbuySelectedSize');
        		$.each(elements, function() {
        			var that = $(this);
        			var quickbuy = new Rocket.plugin.quickbuy(that, {});
        		});
        	}
        },

        sizeSelectedForBuy: function() {
        	var popUp = this.el.find('.size-select-box'),
        		sizeElement = popUp.find('.size-li'),
        		skuInput = this.el.find('#selectedSku'),
        		buyNowForm = this.el.find('#form-cart'),
        		buyNow = this.el.find('.data-buynow');

        	//var quickbuy = new Rocket.plugin.quickbuy($('.quickbuyAc'), {});

        	if (sizeElement.length > 0) {
        		sizeElement.on('hover', function() {
					var that = $(this),
        				sizeSku = that.data('sku-simple');
        			sizeElement.removeClass('active');
        			that.addClass('active');

        			skuInput.val(sizeSku);
        		});

        		sizeElement.on('click', function() {
        			/*var that = $(this),
        				sizeSku = that.data('sku');
        			sizeElement.removeClass('active');
        			that.addClass('active');

        			skuInput.val(sizeSku);*/
        			popUp.addClass('hide');
        			buyNow.removeClass('buynow-inactive');
        		});
        	}
        },

        makeCurrencyFormat: function(price) {
        	var cfventure = {
		        "ID": {
		            decimal: 0,
		            tail: ',',
		            three: '.'
		        },
		        "MY": {
		            decimal: 2,
		            tail: '.',
		            three: ','
		        },
		        "PH": {
		            decimal: 2,
		            tail: '.',
		            three: ','
		        },
		        "TH": {
		            decimal: 0,
		            tail: '.',
		            three: ','
		        },
		        "VN": {
		            decimal: 0,
		            tail: ',',
		            three: '.'
		        },
		        "SG": {
		            decimal: 2,
		            tail: '.',
		            three: ','
		        }
		    };
		    var currency = cfventure[store.venture];
		    if (price) {
		    	return price.formatMoneyExt(currency.decimal, currency.tail, currency.three);
		    } else {
		    	return price;
		    }
        },

        goToSlide: function(activeProduct) {
            if (activeProduct.length > 0 && activeProduct.hasClass('active') === true) {
                var index = activeProduct.data('index'),
                    totalItem = this.ls.find('.hdc-item').length,
                    slideNumber = 0;
                if (index <= totalItem) {
                    if (index < 4) {
                        slideNumber = 0;
                    } else if (index < 8) {
                        slideNumber = 1;
                    } else if (index < 12) {
                        slideNumber = 2;
                    } else if (index < 16) {
                        slideNumber = 3;
                    }
                    if (typeof slider !== 'undefined') {
                        slider.goToSlide(slideNumber);
                    }
                }
            }
        },

        updateProductItemDescription: function(activeProduct) {
            var descriptionWrapper = $('.product-description-wrapper'),
                itemDescription, limit = 4;
            if (activeProduct.length > 0 && descriptionWrapper.length > 0) {
                itemDescription = activeProduct.find('.product-item-description');
                if (itemDescription.length > 0) {

                    var liItem = itemDescription.find('li');
                    if (liItem.length > 0 && liItem.length >= limit) {
                        for (var i = 0; i < liItem.length; i++) {
                            if (i >= limit) {
                                $(liItem[i]).addClass('hide');
                            }
                        }
                    }

                    descriptionWrapper.html(itemDescription.html());
                }
            }
        },

        displayOutOfStock: function(activeProduct) {
            var me = this, hasOutOfStock = activeProduct.find('.item-outofstock'),
                buyNowButton = this.el.find('.data-buynow'),
                buyNowButtonMobile = this.el.find('.btn-buy-now-button-mobile'),
                outOfStockButton = this.el.find('.outOfStock');
            if (hasOutOfStock.length > 0) {
                buyNowButton.addClass('hide');
                buyNowButtonMobile.addClass('hide');
                outOfStockButton.removeClass('hide');
            } else {
            	if (me.ismobile() === false) {
            		me.showBuyNowButtonForActiveProduct(activeProduct);
            	} else {
            		buyNowButtonMobile.removeClass('hide');
            	}
            }
        },

        showBuyNowButtonForActiveProduct: function(activeProduct) {
        	if (activeProduct.length > 0) {
        		var buyNowButton = this.el.find('.data-buynow'),
        			outOfStockButton = this.el.find('.outOfStock');
        			//buyNowActive = $('#buy-wrapper').find('.item-' + activeProduct.data('index')),
        			
        		//buyNowButton.addClass('hide');
        		buyNowButton.removeClass('hide');
        		outOfStockButton.addClass('hide');
        		/*if (buyNowActive.length > 0) {
        			buyNowActive.removeClass('hide');
        		}*/
        	}
        },

        ismobile: function() {
            return $('body.mobile').length > 0;
        },

        resizeHighlighBox: function() {
            var me = this;
            if (me.ismobile() === false) {
                return false;
            }
            
            $(window).resize(function() {
                var activeProduct = me.ls.find('.hdc-item.active');
                if (activeProduct.length > 0) {
                    var highlightBox = activeProduct.find('.item-border');
                    if (highlightBox.length > 0) {
                        highlightBox.width(activeProduct.width() - 4);
                        highlightBox.height(activeProduct.height() - 4);
                    }
                }
            });
        },

        makeProductDescriptionStyle: function() {
            var me = this,
                desList = $('.product-description-mobile').find('li'),
                index = 0;

            if (desList.length > 0) {
                for (index; index < desList.length; index+=2) {
                    desList.slice(index, index+2).wrapAll("<div class='li-wrap'></div>");
                }
            }
        },

        updateHourlyOnHeader: function(productItem) {
            var me = this;
            if (productItem.length > 0) {
                hdcCurrentTimeElement = $('.hdc-current-time');
                hdcCurrentTimeElement.find('.c-text').html(productItem.find('.h-hourly').html());
                hdcCurrentTimeElement.find('.c-unit').html(productItem.find('.h-precise').html());
            }
        },

        activeProductItem: function(productItem) {
            var me = this.el,
                hotDealList = this.ls.find('.hdc-item'),
                popUp = this.el.find('.size-select-box');
            hotDealList.removeClass('active');
            productItem.addClass('active');
			popUp.addClass('hide');
        },

        highlightBoxSize: function(activeProduct) {
            if (activeProduct.length > 0) {
                var highlightBox = activeProduct.find('.item-border');
                if (highlightBox.length > 0) {
                    highlightBox.width(activeProduct.width() - 4);
                    highlightBox.height(activeProduct.height() - 4);
                }
            }
        },

        highlightProductItem: function(activeProduct) {
            var me = this, productList = $('.hdc-list-hot-deal-product').find('.hdc-item'),
                activeHour = 0;

            if (activeProduct.length > 0) {
                activeHour = activeProduct.data('hour');
                $.each(productList, function() {
                    var that = $(this);
                    if (that.hasClass('active') === false && that.data('hour') < activeHour) {
                        that.addClass('highlight');
                    }
                });
            }
        },

        productUrlProcess: function(activeProduct) {
            var hotdealArea = $('.hot-deal-calendar-container'),
                productUrl = hotdealArea.find('.hdc-product-url'),
                countDownArea = hotdealArea.find('.promo-timer__wrap'),
                buynowArea = hotdealArea.find('#buy-wrapper'),
                activeProductATag;
            if (activeProduct.length > 0) {
                activeProductATag = activeProduct.find('.hdc-wrap');
                if (activeProductATag.length > 0) {
                    var activeProductUrl = activeProductATag.attr('href');
                    productUrl.attr('href', activeProductUrl);
                }
            }

            productUrl.on('click', function(evt) {
                var clickTarget = $(evt.target);

                if (clickTarget.hasClass('promo-timer__wrap') === true ||
                    clickTarget.hasClass('instock') === true) {
                    return false;
                }

                if (clickTarget.closest('.promo-timer__wrap').length > 0) {
                    return false;
                }

                if (clickTarget.closest('.instock').length > 0) {
                    return false;
                }
                if (clickTarget.closest('.size-ul').length > 0) {
                    return false;
                }
            });
        }
    };

if (hotDeal.ismobile() === false) {
    	var breadscrumb = $('.breadcrumb-landing');
    	/*,activeItem = breadscrumb.find('.active').addClass('hide')*/
	    $('.header__breadcrumb__wrapper').html(breadscrumb.html());
	}/* else {
		var breadscrumb = $('.campain-12-12-navigation');
			breadscrumb.find('.active').addClass('hide');
	}*/
</script>
