(function(){
    $(".header .header__breadcrumb").html($("#hidden_fasion_menu").html());
	var $fhmi = $(".fashion-header__menu__item");

	$(document).hover(function(){
		$fhmi.removeClass('active');
	});

	$fhmi.click(function(){
		var that = this;
		setTimeout(function () {
			$(that).addClass("active");
		}, 50);
	});

